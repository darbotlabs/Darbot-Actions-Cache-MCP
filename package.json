{
  "name": "@darbot/dac",
  "type": "module",
  "version": "1.0.0",
  "description": "Darbot Actions Cache - High-performance self-hosted cache server for GitHub Actions",
  "author": "Darbot Team",
  "license": "MIT",
  "homepage": "https://github.com/darbotlabs/Darbot-Actions-Cache-MCP",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darbotlabs/Darbot-Actions-Cache-MCP.git"
  },
  "bugs": {
    "url": "https://github.com/darbotlabs/Darbot-Actions-Cache-MCP/issues"
  },
  "bin": {
    "darbot-dac": "./dist/cli/index.js",
    "darbot-dac-mcp": "./dist/mcp/index.js",
    "dar-act-cache": "./dist/cli/index.js"
  },
  "keywords": [
    "darbot",
    "dac",
    "actions-cache",
    "github-actions",
    "cache",
    "ci-cd",
    "mcp",
    "model-context-protocol"
  ],
  "private": true,
  "engines": {
    "node": "22",
    "pnpm": "10"
  },
  "scripts": {
    "prepare": "nitropack prepare && [ -z \"$CI\" ] && husky || echo 'Skipping husky in CI'",
    "dev": "nitropack dev",
    "build": "nitropack build && pnpm build:cli",
    "build:cli": "mkdir -p dist/cli dist/mcp && cp src/cli/index.ts dist/cli/index.js && cp src/mcp/index.ts dist/mcp/index.js",
    "preview": "node .output/server/index.mjs",
    "lint": "eslint --cache . && prettier --check --cache .",
    "lint:ci": "eslint --cache --cache-strategy content . && prettier --check --cache --cache-strategy content .",
    "lint:fix": "eslint --fix --cache . && prettier --write --cache .",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "action": "act -v --env ACTIONS_RESULTS_URL=http://host.docker.internal:3000/ --container-architecture linux/amd64 -W tests",
    "test:watch": "DEBUG=true VITEST_DB_DRIVER=sqlite VITEST_STORAGE_DRIVER=filesystem vitest --watch --ui",
    "test:run": "vitest run"
  },
  "changelogithub": {
    "extends": "gh:darbot/configs/changelogithub"
  },
  "dependencies": {
    "@antfu/utils": "^9.2.0",
    "@aws-sdk/client-s3": "^3.846.0",
    "@aws-sdk/lib-storage": "^3.846.0",
    "@aws-sdk/s3-request-presigner": "^3.846.0",
    "@google-cloud/storage": "^7.16.0",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/pg": "^8.15.4",
    "better-sqlite3": "^12.2.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "consola": "^3.4.2",
    "croner": "^9.1.0",
    "execa": "^9.6.0",
    "h3": "^1.15.3",
    "kysely": "^0.28.2",
    "mysql2": "^3.14.2",
    "nitropack": "^2.12.2",
    "pg": "^8.16.3",
    "remeda": "^2.26.0",
    "typescript": "^5.8.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@actions/cache": "^4.0.3",
    "@commitlint/cli": "^19.8.1",
    "@louishaftmann/commitlint-config": "^4.2.0",
    "@louishaftmann/eslint-config": "^4.2.0",
    "@louishaftmann/lintstaged-config": "^4.2.0",
    "@louishaftmann/prettier-config": "^4.2.0",
    "@testcontainers/mysql": "^11.2.1",
    "@testcontainers/postgresql": "^11.2.1",
    "@types/node": "^22.13.0",
    "@types/wait-on": "^5.3.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "execa": "^9.3.1",
    "husky": "^9.1.7",
    "ky": "^1.8.2",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.0.12",
    "testcontainers": "^11.2.1",
    "ts-pattern": "^5.7.1",
    "tsx": "^4.20.3",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "cpu-features",
      "esbuild",
      "ssh2"
    ]
  }
}
